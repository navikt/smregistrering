{
    "name": "smregistrering-server",
    "version": "1.0.0",
    "repository": "github.com/navikt/smregistrering",
    "description": "Static file and proxy server for registration of papirsykmelding",
    "license": "ISC",
    "main": "server.js",
    "scripts": {
        "build": "rimraf dist && ncc build src/server.ts -o dist",
        "watch:build": "ncc build src/server.ts -w -o dist",
        "watch:run": "nodemon dist/index.js",
        "start": "NODE_ENV=development concurrently \"yarn:watch:*\"",
        "lint": "eslint 'src/**/**.{ts,tsx,js,jsx}'",
        "test": "jest"
    },
    "devDependencies": {
        "@types/express": "^4.17.13",
        "@types/express-http-proxy": "^1.6.3",
        "@types/express-session": "^1.17.4",
        "@types/jest": "^26.0.24",
        "@types/node": "^14.18.12",
        "@types/passport": "^1.0.7",
        "@types/redis": "^2.8.32",
        "@types/tunnel": "0.0.3",
        "@typescript-eslint/eslint-plugin": "^5.12.1",
        "@typescript-eslint/parser": "^5.12.1",
        "@vercel/ncc": "^0.33.3",
        "concurrently": "^7.0.0",
        "eslint": "^8.9.0",
        "eslint-plugin-import": "^2.25.4",
        "jest": "26.6.0",
        "nodemon": "^2.0.15",
        "rimraf": "^3.0.2",
        "ts-jest": "^26.5.6",
        "typescript": "^4.5.5"
    },
    "dependencies": {
        "connect-redis": "^5.2.0",
        "dotenv": "^8.6.0",
        "express": "^4.17.3",
        "express-http-proxy": "^1.6.3",
        "express-session": "^1.17.2",
        "helmet": "^4.6.0",
        "openid-client": "^3.15.10",
        "passport": "^0.4.1",
        "redis": "^3.1.2",
        "tunnel": "0.0.6",
        "winston": "^3.6.0",
        "zod": "^3.11.6"
    }
}
